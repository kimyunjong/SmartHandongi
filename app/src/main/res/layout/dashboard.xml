<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"  tools:context=".MainActivity"
    android:background="#ffd3eaff">
//상단바
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="75dp"
    android:layout_alignParentTop="true"
    android:background="#fffefafc"
    android:orientation="vertical"
        android:id="@+id/upper_bar">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="60"

        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <Button
                android:id="@+id/board_btn"
                android:layout_width="140dp"
                android:layout_height="60dp"
                android:background="#fffefafc"
                />
            <ImageView
                android:id="@+id/board_img"
                android:layout_height="60dp"
                android:layout_width="140dp"
                android:src="@drawable/board_btn_on"/>
            <Button

                android:id="@+id/timeline_btn"
                android:layout_toRightOf="@+id/board_btn"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:background="#fffefafc"

                />
            <ImageView
                android:id="@+id/timeline_img"
                android:layout_height="60dp"
                android:layout_width="40dp"
                android:layout_toRightOf="@+id/board_img"
                android:src="@drawable/board_btn"/>
            <Button
                android:id="@+id/search_btn"
                android:layout_width="45dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/menu_img"
                android:background="#fffefafc"/>
            <ImageView
                android:id="@+id/search_img"
                android:layout_width="45dp"
                android:layout_height="60dp"
                android:layout_alignLeft="@+id/menu_img"
                android:src="@drawable/search_btn"/>

            <Button
                android:id="@+id/menu_btn"
                android:layout_width="45dp"
                android:layout_height="60dp"
                android:layout_alignParentRight="true"
                android:background="#fffefafc"/>
            <ImageView
                android:id="@+id/menu_img"
                android:layout_width="45dp"
                android:layout_height="60dp"
                android:layout_alignParentRight="true"
                android:src="@drawable/menu_btn"/>

        </RelativeLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        android:background="#ffcececf"

        ></LinearLayout>

</LinearLayout>

    //리스트
    <ListView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/list"
    android:layout_below="@+id/upper_bar"
        android:scrollbars="none"
        >

    </ListView>

    //리스트 2 전체
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/upper_bar"
        android:id="@+id/list_container"

       >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:id="@+id/list2_upperbar">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="텍스트입니다. 여기에는 이미지가 올 것입니다."/>
            </LinearLayout>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/list2"
        android:layout_below="@+id/list2_upperbar"
        android:dividerHeight="0dp"
        android:divider="#ffffff"
        android:scrollbars="none">

    </ListView>
    </RelativeLayout>


    //write버튼
    <RelativeLayout

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/downbar"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="8dp">

        <ImageButton

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/write_btn"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="15dp"
            android:src="@drawable/write_btn"/>

    </RelativeLayout>

    //아래하단
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"
        android:id="@+id/downbar"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:background="#ffffffff"
            ></LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="97"
            android:background="#ff7fc3fe">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="16.6"
                >
                <Button
                    android:id="@+id/notice_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#fff"/>
                <ImageView
                    android:id="@+id/notice_img"
                    android:layout_width="40dp"
                    android:layout_height="60dp"
                    android:src="@drawable/notice_btn_on"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"

                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="16.6"
                >
                <Button
                    android:id="@+id/outer_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#fff"/>
                <ImageView
                    android:id="@+id/outer_img"
                    android:layout_width="40dp"
                    android:layout_height="60dp"
                    android:src="@drawable/outer_btn_on"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"

                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="16.6"
                >
                <Button
                    android:id="@+id/seminar_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#fff"/>
                <ImageView
                    android:id="@+id/seminar_img"
                    android:layout_width="40dp"
                    android:layout_height="60dp"
                    android:src="@drawable/seminar_btn_on"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"

                    /></RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="16.6"
                >
                <Button
                    android:id="@+id/recruit_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#fff"/>
                <ImageView
                    android:id="@+id/recruit_img"
                    android:layout_width="40dp"
                    android:layout_height="60dp"
                    android:src="@drawable/recruit_btn_on"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"

                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="16.6">

                <Button
                    android:id="@+id/agora_btn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#fff"/>
                <ImageView
                    android:id="@+id/agora_img"
                    android:layout_width="40dp"
                    android:layout_height="60dp"
                    android:src="@drawable/agora_btn_on"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"

                    /></RelativeLayout>


        </LinearLayout>


    </LinearLayout>

<RelativeLayout
    android:layout_width="130dp"
    android:layout_height="match_parent"
    android:layout_alignParentRight="true"
    android:layout_below="@+id/upper_bar"
    android:background="#000000"
    android:visibility="gone"
    android:id="@+id/menu"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/write_btn_in_menu"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/write_btn_in_menu_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scrap_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/scrap_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/my_posting_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/my_posting_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/push_set_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/push_set_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/show_group"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/show_group_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/bustime_table_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/bustime_table_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/error_report_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/error_report_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ourstory_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ourstory_img"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/other_handongi_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/other_handongi_img"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/logout_btn"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/logout_img"/>
        </RelativeLayout>

    </LinearLayout>
</RelativeLayout>
</RelativeLayout>
